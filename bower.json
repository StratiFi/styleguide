{
  "name": "StratiFi Style Guide",
  "version": "0.1.0",
  "authors": [
    "Savvaoff <savvaoff@gmail.com>"
  ],
  "description": "Stratifi Style Guide",
  "homepage": "stratifi.com",
  "ignore": [
    ".*",
    "*.md",
    "**/.*",
    "**/*.json",
    "node_modules",
    "bower_components",
